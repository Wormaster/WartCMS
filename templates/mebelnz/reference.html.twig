{% extends 'base.html.twig' %}    {% block title %}        <title>{{ cat.seotitle }} - {{ sitename }}</title>    {% endblock %}{% block headimage %}    <div class="page_image right"><img src="/templates/{{ templatefolder }}/images/article_img.jpg" alt=""></div>{% endblock %}{% block crumbs %}    <div class="breadcrumbs block">        <a href="/">Главная</a> <span class="sep">></span>        <span class="sep">{{ cat.name }}</span>    </div>{% endblock %}{% block sectionclass %}text_page references{% endblock %}{% block content %}		<div class="articles_list">			<div class="block_title">{{ cat.name }}</div>			{% for item in items %}                <div class="article">                    <div class="info left">                        <div class="name"><a href="/{{ cat.alias }}/{{ item.alias }}">{{ item.name }}</a></div>                        <div class="desc">                            <p>{{ item.otziv | cut(200) | raw }}</p>                            <a href="/{{ cat.alias }}/{{ item.alias }}" class="details right">Читать дальше</a>                            <div class="clear"></div>                        </div>                    </div>                    <div class="image right"><a href="/{{ cat.alias }}/{{ item.alias }}"><img src="images/{{ item.imagefolder }}/thumbs/{{ item.clientphoto.0.thumb }}" alt="{{ item.name }}" width="84" height="84" /></a></div>                    <div class="clear"></div>                </div>                {% else %}                <p>К сожалению в этом разделе пока ничего нет.</p>                {% endfor %}		</div>{% endblock %}{% block reviews %}{% endblock %}