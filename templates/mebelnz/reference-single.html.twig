{% extends 'base.html.twig' %}    {% block title %}        {% if item.seotitle != '' %}            <title>{{ item.seotitle }}</title>        {% else %}            <title>{{ item.name }} - {{ sitename }}</title>        {% endif %}    {% endblock %}{% block headimage %}		<div class="page_image right"><img src="/templates/{{ templatefolder }}/images/article_img.jpg" alt=""></div>{% endblock %}{% block crumbs %}    <div class="breadcrumbs block">        <a href="/">Главная</a> <span class="sep">></span>        {% if cat.alias != 'static' %}<a href="/{{ cat.alias }}">{{ cat.name }}</a><span class="sep">></span>{% endif %} {{ item.name }}    </div>{% endblock %}{% block sectionclass %}text_page{% endblock %}{% block content %}    <div class="text_block">        <h1>{{ item.name }}</h1>        <p>{{ item.otziv | raw }}</p>    </div>    {% if cat.alias != 'static' %}    <div class="article_links">        <a href="/{{ cat.alias }}/{{ neighbours.prev.alias }}" class="link_prev left"><img src="/templates/{{ templatefolder }}/images/prev_link_icon.png" alt=""> ПРЕДЫДУЩАЯ</a>        <a href="/{{ cat.alias }}/{{ neighbours.next.alias }}" class="link_next right">Следующая <img src="/templates/{{ templatefolder }}/images/next_link_icon.png" alt=""></a>        <a href="/{{ cat.alias }}" class="back">К отзывам</a>        <div class="clear"></div>    </div>    {% endif %}{% endblock %}